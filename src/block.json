{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wpsl/card",
  "title": "WPSL Card",
  "version": "1.0.1",
  "category": "widgets",
  "icon": "star-filled",
  "description": "WPSL generic card block.",
  "supports": {
	  "html": false
  },
  "textdomain": "wpsl-card",

  "attributes": {
	"imageId": {
	  "type": "integer"
	},
	"imageThumbUrl": {
	  "type": "string"
	},
	"title": {
	  "type": "string"
	},
	"url": {
	  "type": "string"
	}
  },

  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
